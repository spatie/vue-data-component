{
  "name": "vue-data-component",
  "version": "1.0.0-alpha.36",
  "description": "A straightforward Vue component to filter, sort and paginate data",
  "browser": "dist/vue-data-component.js",
  "main": "dist/vue-data-component.common.js",
  "module": "dist/vue-data-component.esm.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepublish": "npm run build",
    "lint": "eslint src --fix",
    "format": "prettier --write 'src/**/*.{js,vue}' 'tests/**/*.{js,vue}' 'example/**/*.{js,vue}'",
    "test": "jest",
    "size": "size-limit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spatie/vue-data-component.git"
  },
  "keywords": [
    "spatie"
  ],
  "author": "Sebastian De Deyne",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spatie/vue-data-component/issues"
  },
  "homepage": "https://github.com/spatie/vue-data-component",
  "peerDependencies": {
    "vue": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-syntax-jsx": "^7.0.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-core": "^7.0.0-bridge",
    "babel-eslint": "^9.0.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-plugin-transform-vue-jsx": "^4.0.1",
    "eslint": "^4.19.0",
    "eslint-config-prettier": "^2.9.0",
    "jest": "^23.6.0",
    "prettier": "^1.13.0",
    "rollup": "^0.60.1",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-uglify": "^4.0.0",
    "size-limit": "^0.18.2",
    "vue": "^2.5.17",
    "vue-template-compiler": "^2.5.17"
  },
  "resolutions": {
    "babel-core": "^7.0.0-bridge"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/fixtures/"
    ]
  },
  "size-limit": [
    {
      "limit": "10 KB",
      "path": "dist/vue-data-component.min.js"
    }
  ],
  "dependencies": {
    "deepmerge": "^2.1.1",
    "qs": "^6.5.2"
  }
}
